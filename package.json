{
    "name": "eslint-plugin-handle-errors",
    "version": "0.2.1",
    "description": "ESLint rules for handling errors",
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin",
        "errors",
        "logging"
    ],
    "author": "Maksim Zemskov <maxaz74@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nodge/eslint-plugin-handle-errors/issues"
    },
    "homepage": "https://github.com/nodge/eslint-plugin-handle-errors",
    "repository": {
        "type": "git",
        "url": "https://github.com/nodge/eslint-plugin-handle-errors"
    },
    "engines": {
        "node": ">=18"
    },
    "types": "./dist/index.d.ts",
    "exports": {
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "require": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "*.md",
        "LICENSE",
        "dist"
    ],
    "scripts": {
        "build": "tsup-node",
        "build:watch": "tsup-node --watch",
        "lint": "eslint .",
        "fmt": "prettier --write .",
        "test": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {},
    "devDependencies": {
        "@eslint/js": "9.8.0",
        "@types/eslint": "9.6.0",
        "dedent": "^1.5.3",
        "eslint": "9.8.0",
        "eslint-plugin-eslint-plugin": "6.2.0",
        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "8.0.0n",
        "vitest": "^1.5.0"
    },
    "peerDependencies": {
        "eslint": ">=7"
    }
}
